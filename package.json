{
  "name": "crud-api",
  "version": "1.0.0",
  "description": "Simple CRUD API based on node js",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:dev": "nodemon src/index.ts",
    "start:multi": "cross-env NODE_ENV=cluster nodemon src/clusterServer.ts",
